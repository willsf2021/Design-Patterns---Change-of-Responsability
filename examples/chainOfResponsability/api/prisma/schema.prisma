generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
}

model Categoria {
  id Int @id @default(autoincrement())
  nome String
  produtos Produto[]
}

model Produto {
  id Int @id @default(autoincrement())
  nome String
  categoria_id Int
  categoria Categoria @relation(fields: [categoria_id], references: [id])
}